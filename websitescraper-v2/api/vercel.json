{
  "version": 2,
  "functions": {
    "api/scrape.js": {
      "maxDuration": 5
    },
    "api/result.js": {
      "maxDuration": 5
    },
    "api/health.js": {
      "maxDuration": 3
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/api/health" }
  ]
}